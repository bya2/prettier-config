{
  "name": "@bya2/configs",
  "private": true,
  "description": "Sharing configurations.",
  "homepage": "https://github.com/bya2/configs#readme",
  "bugs": "https://github.com/bya2/configs/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/bya2/configs.git"
  },
  "author": "Jinhyeong Lee <byaa1972@gmail.com> (https://github.com/bya2)",
  "type": "module",
  "scripts": {
    "build": "pnpm -r --filter=\".\\packages\\**\" run build",
    "publish": "pnpm -r publish --tag latest",
    "w:eslint": "pnpm --filter=\".\\packages\\eslint-config\"",
    "w:prettier": "pnpm --filter=\".\\packages\\prettier-config\"",
    "w:rollup": "pnpm --filter=\".\\packages\\rollup-config\"",
    "w:ts": "pnpm --filter=\".\\packages\\tsconfig\"",
    "w:tsup": "pnpm --filter=\".\\packages\\tsup-config\""
  },
  "prettier": "@bya2/prettier-config/plugin.packagejson",
  "devDependencies": {
    "@bya2/prettier-config": "workspace:*",
    "@bya2/tsconfig": "workspace:*",
    "@types/node": "20.11.8",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "packageManager": "pnpm@8.9.0"
}
